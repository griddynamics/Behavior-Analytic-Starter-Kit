# Contact definitions
#
# Autogenerated by Chef.

define contactgroup {
  contactgroup_name admins
  alias             Nagios Administrators
  members           <%= @members.join(',') %>
}

<% @admins.each do |admin| -%>
define contact {
    use          generic-contact
    contact_name <%= admin['id'] %>
    <%= "email  #{admin['nagios']['email']}" unless admin['nagios'].nil? || admin['nagios']['email'].nil? %>
}
<% end -%>
